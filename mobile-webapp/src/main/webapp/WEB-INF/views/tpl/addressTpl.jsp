<div id = "addressDiv">
    <div>
        <a href="javaScript:;"  id="closeAddress"><span><img src="/client/img/coinmall/close.png" style="width: 20px;margin-top: 10px;"></span></a>
    </div>
    <div class="addressRow">
        <input type="text" class="province" id="name" placeholder="收货人姓名" style="border-radius:0 " />
    </div>
    <div class="addressRow">
        <input type="text" class="province" id="cellphone" placeholder="手机" />
    </div>
    <div class="addressRow">
        <input type="text" class="province" id="province" placeholder="省" />
    </div>
    <div class="addressRow">
        <input type="text" class="province" id="city" placeholder="市" />
    </div>
    <div class="addressRow">
        <input type="text" class="province" id="detailAddress1" placeholder="详细地址" />
    </div>
    <div class="addressRow">
        <input type="text" class="province" id="detailAddress2" placeholder="详细地址续" />
    </div>
    <div class="addressRow">
        <button id="saveAddress"><span id="warnAddress">确认地址</span></button>
    </div>
    <div class="addressRow">
        <button id="exchange" onclick="exchange();"><span id="">确定兑换</span></button>
    </div>
</div>